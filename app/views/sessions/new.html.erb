<header>
<h1>ログイン画面</h1>
</header>

<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

  <%= f.label :メールアドレス %>
  <%= f.email_field :email %>

  <%= f.label :パスワード %>
  <%= f.password_field :password %>

  <%= f.submit 'ログインする' %>
<% end %>

<footer>
<p>fakebook</p>
</footer>